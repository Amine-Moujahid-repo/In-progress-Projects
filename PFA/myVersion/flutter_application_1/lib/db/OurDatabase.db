CREATE TABLE Client (
  _id INT PRIMARY KEY,
  date DATE,
  Domaine VARCHAR(50),
  NomClient VARCHAR(50),
  PrenomClient VARCHAR(50),
  NomEntreprise VARCHAR(100),
  idIntervention INT,
  idContact INT,
  idFacture INT,
  FOREIGN KEY (idContact) REFERENCES Contact(idContact),
  FOREIGN KEY (idFacture) REFERENCES Facture(idFacture)
);

